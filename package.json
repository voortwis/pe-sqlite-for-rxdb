{
  "name": "pe-sqlite-for-rxdb",
  "version": "0.1.6",
  "description": "Pineapple Electric SQLite RxStorage for RxDB",
  "author": "Pineapple Electric LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pineapple-electric/pe-sqlite-for-rxdb"
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/better-sqlite3": "^7.6.13",
    "acorn": "^8.15.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "default": "./dist/lib.js"
    },
    "./storage-impl-better-sqlite3": {
      "types": "./dist/storage-impl-better-sqlite3.d.ts",
      "default": "./dist/storage-impl-better-sqlite3.js"
    }
  },
  "files": [
    "COPYING",
    "README.md",
    "dist",
    "package.json"
  ],
  "peerDependencies": {
    "better-sqlite3": "^12.2.0",
    "rxdb": "^15.19.0",
    "rxjs": "^7.8.0"
  },
  "pnpm": {
    "overrides": {
      "ws": ">=8.17.1"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "scripts": {
    "clean": "rimraf dist pe-sqlite-for-rxdb-*.tgz",
    "dist": "tsc && vite build",
    "format": "prettier --write .",
    "lint": "eslint && tsc --noEmit",
    "prepare": "husky",
    "test": "vitest --run --typecheck"
  },
  "type": "module"
}
